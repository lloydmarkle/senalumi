{
  "name": "clone-auralux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "nyc mocha",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "mocha": {
    "loader": "ts-node/esm",
    "require": [
      "./src/test/bootstrap.ts"
    ],
    "spec": "**/*.test.ts",
    "recursive": true
  },
  "nyc": {
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "exclude": "**/*.test.ts",
    "extension": ".ts",
    "report-dir": "build/coverage",
    "temp-dir": "build/private/nyc"
  },
  "devDependencies": {
    "@colyseus/ws-transport": "^0.14.21",
    "@sveltejs/pancake": "0.0.18",
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chai": "^4.3.3",
    "@types/color-convert": "^2.0.0",
    "@types/mocha": "^10.0.0",
    "@types/sinon": "^10.0.13",
    "chai": "^4.3.6",
    "color-convert": "^2.0.1",
    "colyseus": "^0.14.24",
    "colyseus.js": "^0.14.13",
    "deepool": "^3.0.1",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "pixi-cull": "^2.2.0",
    "pixi-viewport": "^4.36.0",
    "pixi.js": "^6.5.6",
    "sinon": "^14.0.1",
    "svelte": "^3.55.1",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-select": "^4.4.7",
    "tone": "^14.7.77",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}

{
  "name": "senalumi",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "nyc mocha",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "mocha": {
    "loader": "ts-node/esm",
    "require": [
      "./src/test/bootstrap.ts"
    ],
    "spec": "src/**/*.test.ts",
    "recursive": true
  },
  "nyc": {
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "exclude": "**/*.test.ts",
    "extension": ".ts",
    "report-dir": "build/coverage",
    "temp-dir": "build/private/nyc"
  },
  "devDependencies": {
    "@pixi/filter-shockwave": "^4.2.0",
    "@sveltejs/pancake": "^0.0.18",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/chai": "^4.3.3",
    "@types/color-convert": "^2.0.0",
    "@types/mocha": "^10.0.0",
    "@types/sinon": "^10.0.13",
    "chai": "^4.3.6",
    "color-convert": "^2.0.1",
    "deepool": "^3.0.1",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "pixi-cull": "^2.2.0",
    "pixi-filters": "^5.1.1",
    "pixi-viewport": "^4.36.0",
    "pixi.js": "^6.5.6",
    "sinon": "^14.0.1",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-preprocess": "^6.0.3",
    "svelte-select": "^5.8.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "unique-names-generator": "^4.7.1",
    "vite": "^7.3.0"
  }
}
